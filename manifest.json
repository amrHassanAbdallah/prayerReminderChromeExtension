{
  "manifest_version": 2,
  "name": "PrayerTimes",
  "version": "1.4",
  "description": "This is my extension",
  "content_security_policy": "script-src 'self' 'unsafe-inline' blob: filesystem:;",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "https://api.aladhan.com/*"
  ]
}